{
  "name": "palestine-news",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Independent Palestine news aggregation website",
  "keywords": ["palestine", "news", "journalism", "gaza", "middle-east", "react", "vite", "netlify"],
  "author": "Palestine News Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "scrape": "python3 scraper.py && cp articles.json public/articles.json",
    "deploy": "npm run build && netlify deploy --prod --dir=dist"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "lucide-react": "^0.468.0",
    "date-fns": "^3.6.0",
    "clsx": "^2.1.1",
    "class-variance-authority": "^0.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.1.10",
    "tw-animate-css": "^1.0.1",
    "vite": "^6.0.5",
